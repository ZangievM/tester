extends layout

block content
    nav.navbar.navbar-expand-sm.bg-dark.navbar-dark
            a.navbar-brand(href='/') XOGame BMS
            ul.navbar-nav
                li.nav-item
                    a.nav-link(href='/newTest') Add new Test 
    table
        tr
            td id
            td Device
            td Apk
            td Test Apk
            td Status
            td Result
        for value in testRuns
            tr 
                td= value.id
                td= value.deviceDesc
                td(id='downloadApk_'+value.id onclick=`downloadApk(\'${this.id}\')`)
                    a(href="#")
                        img(src='/images/apk.png', alt='Download Apk' class='apkInside')
                td(id='downloadTestApk_'+value.id onclick=`downloadApk(\'${this.id}\')`)
                    a(href="#")
                        img(src='/images/apk.png', alt='Download Apk' class='apkInside')
                td= value.status
                td 
                    a(id href="#")


    